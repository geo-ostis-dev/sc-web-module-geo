    "url": "git+https://github.com/geo-ostis-dev/sc-web.module.geo.git"
  },
  "author": "Konstantin Mihlyuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/geo-ostis-dev/sc-web.module.geo/issues"
  },
  "homepage": "https://github.com/geo-ostis-dev/sc-web.module.geo#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "jquery-simulate": "^1.0.2",
    "jquery.cookie": "^1.4.1",
    "leaflet": "^1.3.1",
